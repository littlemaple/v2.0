<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/white"
    android:scrollbars="none" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/measure_result_details_time_margin_bottom"
            android:layout_marginLeft="@dimen/measure_result_details_time_margin_left"
            android:layout_marginRight="@dimen/measure_result_details_time_margin_right"
            android:layout_marginTop="@dimen/measure_result_details_time_margin_top"
            android:gravity="center_vertical"
            android:orientation="horizontal" >

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="@dimen/measure_result_details_time_margin_right"
                android:scaleType="centerInside"
                android:src="@drawable/detailsoftheresultsview_ic_time" />

            <TextView
                android:id="@+id/et_result_details_time_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:alpha="@dimen/five_tenth"
                android:textColor="@android:color/black" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="@dimen/measure_result_margin_top"
                android:gravity="center"
                android:orientation="vertical" >

                <ImageView
                    android:id="@+id/et_result_details_flag_iv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:scaleType="centerInside"
                    android:src="@drawable/testresultsview_testresult_graph_normal" />
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/divider_height"
                android:layout_marginTop="@dimen/et_measure_result_margin_top"
                android:background="@color/divider_bottom" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/measure_result_value_high"
                android:layout_margin="@dimen/measure_result_value_margin"
                android:background="@drawable/item_bg_blood"
                android:gravity="center"
                android:orientation="horizontal" >

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:orientation="horizontal" >

                    <TextView
                        android:id="@+id/et_result_details_etTV"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:textColor="@color/font_history_high_priority_value"
                        android:textSize="@dimen/font_audio_measure_result_vlaue"
                        android:textStyle="bold" />

                    <LinearLayout
                        android:layout_width="@dimen/et_measure_result_util_high"
                        android:layout_height="@dimen/et_measure_result_util_high"
                        android:layout_marginLeft="@dimen/et_measure_result_margin_left"
                        android:orientation="horizontal" >

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="10"
                            android:gravity="top|right"
                            android:orientation="horizontal" >

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:alpha="@dimen/two_tenth"
                                android:text="@string/ear_temperature_unit_before"
                                android:textColor="@android:color/black"
                                android:textSize="@dimen/font_audio_measure_result_util_o" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="20"
                            android:gravity="bottom|left"
                            android:orientation="horizontal" >

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:alpha="@dimen/two_tenth"
                                android:text="@string/ear_temperature_unit_after"
                                android:textColor="@android:color/black"
                                android:textSize="@dimen/font_audio_measure_result_util_C" />
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/et_result_details_readmeLL"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/measure_result_value_margin_right"
                android:layout_marginLeft="@dimen/measure_result_value_margin"
                android:layout_marginRight="@dimen/measure_result_value_margin"
                android:background="@drawable/item_readme_bg" >

                <TextView
                    android:id="@+id/et_result_details_readmeTV"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_margin="@dimen/oxygen_result_details_readme_margin"
                    android:background="@drawable/item_et_bg"
                    android:ems="15"
                    android:padding="@dimen/oxygen_result_details_readme_padding"
                    android:textSize="@dimen/font_pressure_measure_change_unit" />
            </LinearLayout>
        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/divider_height"
            android:background="@color/divider_bottom" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/measure_result_details_history_trend_high"
            android:layout_marginLeft="@dimen/measure_result_value_margin"
            android:layout_marginRight="@dimen/measure_result_value_margin"
            android:gravity="center_vertical"
            android:orientation="horizontal" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/nearly_three_times_text"
                android:textSize="@dimen/font_measure_result_details_history_trend_text"
                android:textStyle="bold" />

            <View
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1" />

            <LinearLayout
                android:id="@+id/et_result_details_history_trend"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/selector_press_effect"
                android:gravity="right|center_vertical"
                android:orientation="horizontal" >

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="right"
                    android:scaleType="centerInside"
                    android:src="@drawable/detailsoftheresultsview_ic_historicaltrends" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/trend_text"
                    android:textColor="@color/font_recent_history_enter"
                    android:textSize="@dimen/font_measure_result_details_history_trend_text"
                    android:textStyle="bold" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/measure_result_value_margin"
            android:layout_marginRight="@dimen/measure_result_value_margin"
            android:orientation="vertical" >

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/divider_height"
                android:background="@color/divider_bottom" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/measure_result_details_history_trend_itme_high"
                android:gravity="center_vertical"
                android:orientation="horizontal" >

                <ImageView
                    android:id="@+id/et_result_details_history_trend_image_one"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="@dimen/measure_result_details_time_margin_right"
                    android:scaleType="centerInside"
                    android:src="@drawable/detailsoftheresultsview_ic_time" />

                <TextView
                    android:id="@+id/et_result_details_history_trend_time_one"
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:alpha="@dimen/five_tenth"
                    android:textColor="@android:color/black"
                    android:textSize="@dimen/font_measure_result_details_history_trend_text" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="right|center_vertical"
                    android:orientation="horizontal" >

                    <TextView
                        android:id="@+id/et_result_details_history_trend_value_one"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:alpha="@dimen/five_tenth"
                        android:textColor="@android:color/black"
                        android:textSize="@dimen/font_measure_result_details_history_trend_text" />
                </LinearLayout>
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/divider_height"
                android:background="@color/divider_bottom" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/measure_result_details_history_trend_itme_high"
                android:gravity="center_vertical"
                android:orientation="horizontal" >

                <ImageView
                    android:id="@+id/et_result_details_history_trend_image_two"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="@dimen/measure_result_details_time_margin_right"
                    android:scaleType="centerInside"
                    android:src="@drawable/detailsoftheresultsview_ic_time" />

                <TextView
                    android:id="@+id/et_result_details_history_trend_time_two"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:alpha="@dimen/five_tenth"
                    android:textColor="@android:color/black"
                    android:textSize="@dimen/font_measure_result_details_history_trend_text" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="right|center_vertical"
                    android:orientation="horizontal" >

                    <TextView
                        android:id="@+id/et_result_details_history_trend_value_two"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:alpha="@dimen/five_tenth"
                        android:textColor="@android:color/black"
                        android:textSize="@dimen/font_measure_result_details_history_trend_text" />
                </LinearLayout>
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/divider_height"
                android:background="@color/divider_bottom" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/measure_result_details_history_trend_itme_high"
                android:gravity="center_vertical"
                android:orientation="horizontal" >

                <ImageView
                    android:id="@+id/et_result_details_history_trend_image_three"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="@dimen/measure_result_details_time_margin_right"
                    android:scaleType="centerInside"
                    android:src="@drawable/detailsoftheresultsview_ic_time" />

                <TextView
                    android:id="@+id/et_result_details_history_trend_time_three"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:alpha="@dimen/five_tenth"
                    android:textColor="@android:color/black"
                    android:textSize="@dimen/font_measure_result_details_history_trend_text" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="right|center_vertical"
                    android:orientation="horizontal" >

                    <TextView
                        android:id="@+id/et_result_details_history_trend_value_three"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:alpha="@dimen/five_tenth"
                        android:textColor="@android:color/black"
                        android:textSize="@dimen/font_measure_result_details_history_trend_text" />
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/divider_height"
            android:background="@color/divider_bottom" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/measure_result_value_margin"
            android:layout_marginRight="@dimen/measure_result_value_margin"
            android:orientation="vertical" >

            <LinearLayout
                android:id="@+id/et_result_details_suggestLL"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical" >

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/measure_result_details_suggest_title_high"
                    android:gravity="center_vertical"
                    android:text="@string/suggess_text"
                    android:textSize="@dimen/font_measure_result_details_history_trend_text"
                    android:textStyle="bold" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/divider_height"
                    android:background="@color/divider_bottom" />
            </LinearLayout>

            <com.medzone.cloud.ui.widget.CloudWebView
                android:id="@+id/et_result_details_suggestWV"
                android:layout_width="match_parent"
                android:layout_height="match_parent" >
            </com.medzone.cloud.ui.widget.CloudWebView>
        </LinearLayout>
    </LinearLayout>

</ScrollView>