<?xml version="1.0" encoding="utf-8"?><LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="match_parent"    android:layout_height="wrap_content"    android:background="#EBF0F0"    android:orientation="horizontal" >    <TextView        android:id="@+id/tv_top_tip"        android:layout_width="match_parent"        android:layout_height="@dimen/group_member_text_height"        android:layout_marginBottom="@dimen/group_member_text_margin_vertical"        android:layout_marginLeft="@dimen/group_member_margin_horizal"        android:layout_marginRight="@dimen/group_member_margin_horizal"        android:layout_marginTop="@dimen/group_member_text_margin_vertical"        android:gravity="center_vertical"        android:textColor="#AEB3B3"        android:textSize="18sp" /></LinearLayout>